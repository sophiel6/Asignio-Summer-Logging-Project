@using AsignioInternship.Data.LogWebRequest

@model IEnumerable<LogWebRequestDataModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- Added link to search the log
    Also, realized that all the UserID's for LogWebRequest are null, so search by userID maybe isn't needed? -->

@Html.ActionLink("Search", "Search", "LogWebRequest");

<table style="width:100%; text-align:center;">
    <thead>
        <tr>
            <td>Time Stamp</td>
            <td>Web Request ID</td>
            <td>User ID</td>
            <td>HostAddress</td>
            <td>ServerIP</td>
            <td>RawURL</td>
            <td>Parameters</td>
            <td>UserAgent</td>
            <td>UserLanguages</td>

        </tr>

    </thead>
    <tbody>

        @foreach (LogWebRequestDataModel currentRow in Model)
        {
            <tr>
                <td>@currentRow.TimeStamp</td>
                <td>@currentRow.WebRequestID</td>
                <td>@currentRow.UserID</td>
                <td>@currentRow.HostAddress</td>
                <td>@currentRow.ServerIP</td>
                <td>@currentRow.RawURL</td>
                <td>@currentRow.Parameters</td>
                <td>@currentRow.UserAgent</td>
                <td>@currentRow.UserLanguages</td>
                <!-- <td> (at) Html.ActionLink("Edit", "Update", new { UserID = currentRow.ExampleID })</td> -->
            </tr>

        }
    </tbody>
</table>

